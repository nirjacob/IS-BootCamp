{
  "env": {
    "es6": true,
    "es2017": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "prettier",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {},
  "plugins": [
    "jest",
    "babel",
    "prettier"
  ],
  "parser": "babel-eslint",
  "globals": {
    "document": "readonly"
  }
}